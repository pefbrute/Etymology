import subprocess

python_script_path = "/home/pefbrute/.config/autokey/data/My\ Phrases/Скрипты\ и\ прочее1/Сложные\ Скрипты\ Запуск\ через\ subprocess/Этимология/С\ передачей\ аргументов\ через\ консоль1/Выполнение\ Программы\ с\ принятием\ аргумента.py"
python_command = "python3"
delay_time = "6.5"  # Пример времени задержки в секундах

# Добавление аргумента времени задержки в командуы
execution_command = f"{python_command} {python_script_path} {delay_time}"

subprocess.run(execution_command, shell=True)
